{
	"name": "@noctisatrae/solana-tx-parser",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"author": {
		"name": "Hector",
		"url": "https://github.com/noctisatrae"
	},
	"keywords": [
		"solana",
		"web3",
		"parser"
	],
	"type": "module",
	"private": false,
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/bun": "latest"
	},
	"scripts": {
		"fmt": "biome format --write",
		"lint": "biome lint --write",
		"typecheck": "bun tsc",
		"build": "bun build --production --keep-names src/index.ts --outdir=dist/ && tsc --project tsconfig.build.json"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@solana/kit": "^4.0.0"
	}
